<?xml version="1.0" encoding="utf-8" ?>

<!-- Spawns SCPs in Alpha Biome (mod) biomes. If you don't have the mod, this doesn't do anything. -->
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Biomes</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="SCR_SCP111R"]/race/wildBiomes</xpath>
					<value>
						<AB_IdyllicMeadows>0.4</AB_IdyllicMeadows>
						<AB_GelatinousSuperorganism>0.2</AB_GelatinousSuperorganism>
						<AB_TarPits>0.2</AB_TarPits>
						<AB_GallatrossGraveyard>0.1</AB_GallatrossGraveyard>
						<AB_MiasmicMangrove>0.1</AB_MiasmicMangrove>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="SCR_SCP1675R"]/race/wildBiomes</xpath>
					<value>
						<AB_IdyllicMeadows>0.1</AB_IdyllicMeadows>
						<AB_MycoticJungle>0.1</AB_MycoticJungle>
						<AB_GelatinousSuperorganism>0.1</AB_GelatinousSuperorganism>
						<AB_TarPits>0.1</AB_TarPits>
						<AB_OcularForest>0.1</AB_OcularForest>
						<AB_GallatrossGraveyard>0.1</AB_GallatrossGraveyard>
						<AB_MechanoidIntrusion>0.2</AB_MechanoidIntrusion>
						<AB_PropaneLakes>0.1</AB_PropaneLakes>
						<AB_RockyCrags>0.1</AB_RockyCrags>
						<AB_MiasmicMangrove>0.1</AB_MiasmicMangrove>
						<AB_FeraliskInfestedJungle>0.1</AB_FeraliskInfestedJungle>
						<AB_PyroclasticConflagration>0.1</AB_PyroclasticConflagration>
					</value>
				</li>	
			 	<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="SCR_SCP1797A"]/race/wildBiomes</xpath>
					<value>
						<AB_MiasmicMangrove>0.2</AB_MiasmicMangrove>
					</value>
				</li>		
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="SCR_SCP250R"]/race/wildBiomes</xpath>
					<value>
						<AB_TarPits>0.3</AB_TarPits>
						<AB_GallatrossGraveyard>0.1</AB_GallatrossGraveyard>
						<AB_MiasmicMangrove>0.2</AB_MiasmicMangrove>
						<AB_FeraliskInfestedJungle>0.1</AB_FeraliskInfestedJungle>
					</value>
				</li>	
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="SCR_SCP2584R"]/race/wildBiomes</xpath>
					<value>
						<AB_IdyllicMeadows>0.3</AB_IdyllicMeadows>
						<AB_GelatinousSuperorganism>0.2</AB_GelatinousSuperorganism>
						<AB_TarPits>0.2</AB_TarPits>
						<AB_FeraliskInfestedJungle>0.2</AB_FeraliskInfestedJungle>
					</value>
				</li>	
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="SCR_SCP5185R"]/race/wildBiomes</xpath>
					<value>
						<AB_IdyllicMeadows>0.1</AB_IdyllicMeadows>
						<AB_GelatinousSuperorganism>0.2</AB_GelatinousSuperorganism>
						<AB_TarPits>0.1</AB_TarPits>
						<AB_MiasmicMangrove>0.1</AB_MiasmicMangrove>
						<AB_FeraliskInfestedJungle>0.5</AB_FeraliskInfestedJungle>
					</value>
				</li>	
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="SCR_SCP529R"]/race/wildBiomes</xpath>
					<value>
						<AB_IdyllicMeadows>0.1</AB_IdyllicMeadows>
					</value>
				</li>	
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="SCR_SCP5893R"]/race/wildBiomes</xpath>
					<value>
						<AB_GelatinousSuperorganism>0.01</AB_GelatinousSuperorganism>
						<AB_TarPits>0.2</AB_TarPits>
						<AB_PropaneLakes>0.2</AB_PropaneLakes>
						<AB_MiasmicMangrove>0.2</AB_MiasmicMangrove>
						<AB_FeraliskInfestedJungle>0.2</AB_FeraliskInfestedJungle>
						<AB_PyroclasticConflagration>0.5</AB_PyroclasticConflagration>
					</value>
				</li>	
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="SCR_SCP594R"]/race/wildBiomes</xpath>
					<value>
						<AB_GelatinousSuperorganism>0.05</AB_GelatinousSuperorganism>
					</value>
				</li>			
			</operations>
		</match>
	</Operation>
</Patch>
