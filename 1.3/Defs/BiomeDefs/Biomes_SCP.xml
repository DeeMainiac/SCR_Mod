	<li Class="SecureContainRimworld.CProceduresExtension">
	<classRating>
		<li>SCR_CKeter</li>
		<li>SCR_DEkhi</li>
		<li>SCR_RDanger</li>
	</classRating>
	
	<containmentTier>0</containmentTier>
	
	</li>
	</modExtensions>
