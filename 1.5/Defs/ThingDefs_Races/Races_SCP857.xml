			<baseBodySize>1</baseBodySize> <!-- Full length of a human because of the veins it comprises -->
			<baseHungerRate>0.2</baseHungerRate>
			<baseHealthScale>0.4</baseHealthScale>
			<foodType>VegetarianRoughAnimal, Kibble</foodType>
			<wildness>0</wildness>
