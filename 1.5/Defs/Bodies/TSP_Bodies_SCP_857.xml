	<BodyDef>
		<defName>TSP_SCR_857H</defName>
		<label>ribcage</label>
		<corePart>
			<def>Spine</def>
			<height>Top</height>
			<depth>Inside</depth>
			<coverage>0</coverage>
			<groups>
				<li>Neck</li>
			</groups>
			<parts>
				<li>
					<def>Finger</def>
					<customLabel>cervical</customLabel>
					<coverage>0</coverage>
					<height>Top</height>
					<depth>Outside</depth>
					<groups>
						<li>Teeth</li>
					</groups>
				</li>
				<li>
					<def>Brain</def>
					<customLabel>spinal cord</customLabel>
					<coverage>0.05</coverage>
					<height>Bottom</height>
					<depth>Inside</depth>
					<groups>
						<li>Neck</li>
					</groups>
				</li>
				<li>
					<def>Sternum</def>
					<coverage>0.05</coverage>
					<height>Bottom</height>
					<depth>Inside</depth>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				<li>
					<def>Ribcage</def>
					<depth>Outside</depth>
					<height>Top</height>
					<coverage>0.1</coverage>
					<groups>
						<li>HeadClaw</li>
					</groups>
					<parts>
						<!-- Rather than one entry for all twenty-four ribs, I will instead do it per rib group. -->
						<!-- Ribs are classed by their relation to the sternum -->
						<li>
							<def>Ribcage</def>
							<customLabel>vertebrosternal ribs</customLabel> <!-- First seven pairs of ribs -->
							<depth>Inside</depth>
							<coverage>0.4</coverage>
							<groups>
								<li>TuskAttackTool</li>
							</groups>
						</li>
						<li>
							<def>Ribcage</def>
							<customLabel>vertebrochondral ribs</customLabel> <!-- Rib pairs eight through ten, with the final two pairs not present because they are not connected to the sternum or spine, making them unable to manifest in a moving ribcage  -->
							<depth>Inside</depth>
							<coverage>0.4</coverage>
							<groups>
								<li>TuskAttackTool</li>
							</groups>
						</li>
					</parts>
				</li>
				<li>
					<def>TSP_SCR_857_Enchantment</def>
					<depth>Inside</depth>
					<height>Middle</height>
					<coverage>0</coverage>
					<!-- <groups></groups> -->
				</li>
			</parts>
		</corePart>
	</BodyDef>
</Defs>