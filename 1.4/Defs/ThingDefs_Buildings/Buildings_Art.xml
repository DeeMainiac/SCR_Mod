<?xml version="1.0" encoding="utf-8" ?>
<Defs> 
  <ThingDef ParentName="SculptureBase">
    <defName>SCR_SculptureSCP173R</defName>
    <label>SCP-173 Sculpture</label>
    <description>A non-living sculpture of SCP-173. \nSCP-173 is a fast sculpture that, unless it's being watched, will kill pawns who are not looking at it.</description>
    <castEdgeShadows>true</castEdgeShadows>
		<modExtensions>
	<li Class="SecureContainRimworld.CProceduresExtension">

	<classRating>
		<li>SCR_CEuclid</li>
		<li>SCR_DKeneq</li>
		<li>SCR_RNotice</li>
	</classRating>

	<containmentTier>1</containmentTier>
	
	</li>
	</modExtensions>
    <graphicData>
      <texPath>Things/Buildings/Art/SCR_SculptureSCP173</texPath>
	  <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.7</drawSize>
    </graphicData>
    <fillPercent>0.5</fillPercent>
    <rotatable>false</rotatable>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <Mass>7</Mass>
	  <Beauty>10</Beauty>
      <WorkToMake>18000</WorkToMake>
      <TerrorSource MayRequire="Ludeon.RimWorld.Ideology">0</TerrorSource>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
	    <thingCategories>
      <li>SCPObjects</li>
    </thingCategories>
        <costStuffCount>50</costStuffCount>
  </ThingDef>
</Defs>