<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationFindMod">
	<!-- Do any of the following Mods exist? -->
		<mods>
			<li>Expandable Plants (Continued)</li>
			<li>Expandable Plants</li>
		</mods>
		<!-- If it does, do this replacement. -->
		<match Class="PatchOperationReplace">
			<success>Always</success>
			<xpath>/Defs/ThingDef[defName="SCR_SCP848R"]/thingClass</xpath>
			<value>
				<thingClass>ExpandablePlants.Plant</thingClass>
			</value>
		</match>
		<!-- If none of those Mods exist, the original code stands. -->
	</Operation>
</Patch>